class Solution {
public:
    int solveRec(string &s1,string &s2,int i,int j){
        if(i==s1.length() || j==s2.length()){
            return 0;
        }
        int ans=0;
        if(s1[i]==s2[j]){
            ans=s1[i]+solveRec(s1,s2,i+1,j+1);
        }else{
            ans=max(solveRec(s1,s2,i+1,j),solveRec(s1,s2,i,j+1));
        }
        return ans;
    }
	int minimumDeleteSum(string s1, string s2) {
        int n=s1.length();
        int m=s2.length();
        int sum=0;
        for(int i=0;i<n;i++){
            sum+=s1[i];
        }
        for(int i=0;i<m;i++){
            sum+=s2[i];
        }
        vector<vector<int>>dp(n+1,vector)
        return sum-(2*(solveRec(s1,s2,0,0)));
    }
};
